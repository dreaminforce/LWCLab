{
  "name": "my-lwr-app",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules __lwr_cache__ site",
    "dev": "lwr dev",
    "dev:compat": "lwr dev --mode compat",
    "build": "lwr build --clean",
    "build:prod-compat": "lwr build --clean --mode prod-compat",
    "start": "lwr start",
    "stage": "yarn build && yarn start",
    "stage:prod-compat": "yarn build:prod-compat && yarn start",
    "api": "node api/server.mjs",
    "start:all": "concurrently -k \"npm:dev\" \"npm:api\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsforce": "^3.10.7",
    "jszip": "^3.10.1",
    "lwc": "latest",
    "lwr": "0.18.3",
    "openai": "^5.20.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "20.16.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
